<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id='x_contract_form_view' model='ir.ui.view'>

    <field name='name'>x_contract_form.hr.contract.form</field>

    <field name='model'>hr.contract</field>

    <field name='inherit_id' ref='hr_contract.hr_contract_view_form' />

    <field name='arch' type='xml'>
      <field name="structure_type_id" position="attributes">
        <attribute name="invisible">1</attribute>
      </field>

      <xpath expr='//field[@name="structure_type_id"]' position='before'>
        <field name='trial_date_end' string='Fin Periodo de Prueba' />
        <field name='contract_class' string='Lugar de expedicion' />
      </xpath>

    </field>
  </record>
</odoo>