<odoo>
    <!-- Action -->
    <record id="action_hr_contribution_accounts" model="ir.actions.act_window">
        <field name="name">Aportes</field>
        <field name="res_model">hr.contribution.accounts</field>
        <field name="view_mode">list,form</field>
    </record>
    <!-- Form -->
    <record id="view_hr_contribution_accounts_form" model="ir.ui.view">
        <field name="name">hr.contribution.accounts.form</field>
        <field name="model">hr.contribution.accounts</field>
        <field name="arch" type="xml">
            <form string="Cuenta de aporte">
                <sheet>
                    <group>
                        <field name="type" />
                        <field name="name" />
                        <field name="account_id" />
                        <field name="value_type" />
                        <field name="value" />
                        <field name="active" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- List o view -->
    <record id="view_hr_contribution_accounts_list" model="ir.ui.view">
        <field name="name">hr.contribution.accounts.list</field>
        <field name="model">hr.contribution.accounts</field>
        <field name="arch" type="xml">
            <list>
                <field name="type" />
                <field name="name" />
                <field name="account_id" />
                <field name="value" />
                <field name="active" />
            </list>
        </field>
    </record>

    <!-- Menu Item -->
    <menuitem name="Cuentas de Aportes" id="payroll.contribution_accounts"
        parent="payroll.configuration" action='action_hr_contribution_accounts' />

</odoo>