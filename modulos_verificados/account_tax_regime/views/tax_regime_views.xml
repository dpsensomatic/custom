<odoo>
  
  <record id="view_tax_regime_list" model="ir.ui.view">
    <field name="name">x.tax.regime.list</field>
    <field name="model">x.tax.regime</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
        <field name="code"/>
        <field name="tax_ids"/>
      </list>
    </field>
  </record>

  <record id="view_tax_regime_form" model="ir.ui.view">
    <field name="name">x.tax.regime.form</field>
    <field name="model">x.tax.regime</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="code"/>
            <field name="tax_ids" widget="many2many_tags"/>
            <field name="active"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <!-- Acción -->
  <record id="action_tax_regime" model="ir.actions.act_window">
    <field name="name">Régimenes</field>
    <field name="res_model">x.tax.regime</field>
    <field name="view_mode">list,form</field>
  </record>

    <!-- Menús -->
  <menuitem id="menu_tax_regime_root" name="Régimen Fiscal" parent="account.menu_finance"/>
  <menuitem id="menu_tax_regime" name="Régimenes" parent="menu_tax_regime_root" action="action_tax_regime"/>

</odoo>